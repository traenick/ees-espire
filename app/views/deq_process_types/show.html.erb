<%- model_class = DeqProcessType -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:processTypeId) %>:</strong></dt>
  <dd><%= @deq_process_type.processTypeId %></dd>
  <dt><strong><%= model_class.human_attribute_name(:message_sub_type_id) %>:</strong></dt>
  <dd><%= @deq_process_type.message_sub_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:originating_transactionId) %>:</strong></dt>
  <dd><%= @deq_process_type.originating_transactionId %></dd>
  <dt><strong><%= model_class.human_attribute_name(:deq_response_id) %>:</strong></dt>
  <dd><%= @deq_process_type.deq_response_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              deq_process_types_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_deq_process_type_path(@deq_process_type), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              deq_process_type_path(@deq_process_type),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
