<%- model_class = MessageSubType -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Message Type:</strong></dt>
  <dd><%= @message_sub_type.message_type.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sub_type_name) %>:</strong></dt>
  <dd><%= @message_sub_type.sub_type_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:processTypeId) %>:</strong></dt>
  <dd><%= @message_sub_type.processTypeId %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              message_sub_types_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_message_sub_type_path(@message_sub_type), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              message_sub_type_path(@message_sub_type),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

<hr/>


<%= render :partial => 'response_choices' %>