<%= form_for @deq_response, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :response_string, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :response_string, :class => 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :resolved_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :resolved_at, :class => 'form-control' %>
    </div>
  </div>

  
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            deq_responses_path, :class => 'btn btn-default' %>
 
<% end %>
